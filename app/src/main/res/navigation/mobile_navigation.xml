    <fragment
        android:id="@+id/nav_mediciones"
        android:name="com.example.diabetrometrov01.Interfaces.Mediciones.Mediciones"
        android:label="@string/menu_Medidas"
        tools:layout="@layout/fragment_mediciones" />
    <!-- Reportes -->
    <fragment
        android:id="@+id/galleryFragment"
        android:name="com.example.diabetrometrov01.reporte"
        android:label="Reportes generados"
        tools:layout="@layout/fragment_reporte" />
