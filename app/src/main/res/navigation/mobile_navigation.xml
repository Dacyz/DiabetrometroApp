    <!-- Acerca de -->
    <fragment
        android:id="@+id/nav_info"
        android:name="com.example.diabetrometrov01.Interfaces.home.nav_info"
        android:label="@string/acerca_de"
        tools:layout="@layout/fragment_nav_info" />
