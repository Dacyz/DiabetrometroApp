        tools:layout="@layout/fragment_ingesta_alimenticia" />
    <!-- Actividad Fisica -->
    <fragment
        android:id="@+id/nav_ActividadFisica"
        android:name="com.example.diabetrometrov01.Interfaces.ActividadFisica.ActividadFisica"
